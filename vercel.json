{
    "version": 2,
    "builds": [
        {
            "src": "./api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "static/images/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}